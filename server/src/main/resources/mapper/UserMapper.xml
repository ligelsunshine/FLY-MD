<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fly.server.mapper.UserMapper">
    <resultMap id="result" type="com.fly.common.model.entity.UserEntity">
        <id column="id" jdbcType="VARCHAR" property="id"/>
        <result column="userName" property="userName" jdbcType="VARCHAR"/>
        <result column="password" property="passWord" jdbcType="VARCHAR"/>
    </resultMap>
    <insert id="insert" parameterType="com.fly.common.model.entity.UserEntity">
        INSERT INTO user (userName, passWord)
        VALUES(#{userEntity.userName},#{userEntity.passWord})
    </insert>

    <select id="selectUserByName" resultType="com.fly.common.model.entity.UserEntity">
        select * from user where username = #{userName}
    </select>
</mapper>
